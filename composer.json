{
	"name": "philo/laravel-twitter",
	"description": "A simple Laravel 4 service provider for including the TwitterOAuth library.",
	"keywords": ["laravel","laravel-4", "twitter"],
	"homepage": "https://github.com/PhiloNL/Laravel-Twitter",
	"license": "BSD",
	"authors": [
		{
			"name": "Philo Hermans",
			"email": "me@philohermans.com"
		}
	],
	"repositories": [{
	  "type": "vcs",
	  "url": "https://github.com/PhiloNL/TwitterOAuth"
	}],
	"require": {
		"php": ">=5.3.0",
		"illuminate/support": "7.x",
		"tijsverkoyen/twitteroauth": "dev-master"
	},
	"autoload": {
		"psr-0": {
			"Philo\\Twitter": "src/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": false
}
